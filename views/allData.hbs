<h1>{{title}}</h1>

<table>
  <thead>
    <tr>
      <th>Product ID</th>
      <th>Title</th>
      <th>Reviews</th>
      <!-- Add more columns as needed -->
    </tr>
  </thead>
  <tbody>
    {{#each products}}
      
        <tr {{#unless this.reviews}} class="highlighted"{{/unless}}>
          <td>{{this.asin}}</td>
          <td>{{this.title}}</td>
          <td>{{noReviews this.reviews}}</td>
          <!-- Add more columns as needed -->
        </tr>
     
    {{/each}}
  </tbody>
</table>
